<div class="new-restaurant-form">
<h1>Enter a Restaurant</h1>
<%= erb :'_errors' %>
<form action='/restaurants' method='post'>
	<input type="text" name="restaurant[name]" placeholder="Name">
	<input type="text" name="restaurant[address]" placeholder="Address">
	<input type="text" name="restaurant[city]" placeholder="City">
	<input type="text" name="restaurant[state]" placeholder="State">
	<input type="text" name="restaurant[zip]" placeholder="Zip">
	<input type="text" name="restaurant[cuisine]" placeholder="Cuisine">
	<input type="submit" value="ADD" >
</form>
</div>